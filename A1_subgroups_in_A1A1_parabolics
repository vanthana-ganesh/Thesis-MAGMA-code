x1pB:= func < t | elt<GP| <1,t^(2^(r))>,<4,t^(2^(s))>,<5,t^(2^(r-1))*cycle[1]>,<7,t^(2^(s-1))*cycle[2]>,<6,F[14]>,<10,F[15]>,<8,F[16]>,<12,F[17]>,<9,F[18]>,<11,F[19]>,<13,F[20]>,<15,F[21]>,<16,F[22]>,<18,F[23]>,<14,F[24]>,<17,F[25]>,<20,F[26]>>>;
x1pC:= func < t | elt<GP| <1,t^(2^(r))>,<4,t^(2^(s))>,<5,t^(2^(r-1))*cycle[1]>,<7,t^(2^(s-1))*cycle[2]>,<6,F[27]>,<10,F[28]>,<8,F[29]>,<12,F[30]>,<9,F[31]>,<11,F[32]>,<13,F[33]>,<15,F[34]>,<16,F[35]>,<18,F[36]>,<14,F[37]>,<17,F[38]>,<20,F[39]>>>;
